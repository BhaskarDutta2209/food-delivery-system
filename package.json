{
  "name": "food-delivery-system",
  "type": "module",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "pretest": "env-cmd -f ./config/test.env npx sequelize-cli db:drop && env-cmd -f ./config/test.env npx sequelize-cli db:create && env-cmd -f ./config/test.env npx sequelize-cli db:migrate && env-cmd -f ./config/test.env npx sequelize-cli db:seed:all",
    "test": "env-cmd -f ./config/test.env mocha --config ./config/.mocharc.cjs",
    "dev": "env-cmd -f ./config/development.env nodemon src/index.js",
    "start_production": "env-cmd -f ./config/production.env node src/index.js",
    "db:create": "env-cmd -f ./config/$ENV.env npx sequelize-cli db:create",
    "db:drop": "env-cmd -f ./config/$ENV.env npx sequelize-cli db:drop",
    "db:migrate": "env-cmd -f ./config/$ENV.env npx sequelize-cli db:migrate",
    "db:rollback": "env-cmd -f ./config/$ENV.env npx sequelize-cli db:migrate:undo",
    "db:seed:all": "env-cmd -f ./config/$ENV.env npx sequelize-cli db:seed:all",
    "db:seed": "env-cmd -f ./config/$ENV.env npx sequelize-cli db:seed --seed $FILE",
    "db:seed:undo": "env-cmd -f ./config/$ENV.env npx sequelize-cli db:seed:undo"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "express-request-id": "^3.0.0",
    "express-validator": "^7.2.0",
    "express-winston": "^4.2.0",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "redis": "^4.7.0",
    "sequelize": "^6.37.5",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "chai": "^5.1.2",
    "env-cmd": "^10.1.0",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.7",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0"
  }
}
